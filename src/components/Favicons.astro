---
import favIcon from '~/assets/favicons/favicon.ico';
import favIconSvg from '~/assets/favicons/favicon.svg';
import favIconApple from '~/assets/favicons/apple-touch-icon.png';
---

<link rel="shortcut icon" href={favIcon} sizes="32x32" />
<link rel="icon" type="image/svg+xml" href={favIconSvg.src} />
<link rel="apple-touch-icon" href={favIconApple} />
<link rel="manifest" href="/site.webmanifest" />
